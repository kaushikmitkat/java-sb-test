{
  "family": "springboot-app-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::410699729259:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "kaushik-test",
      "image": "410699729259.dkr.ecr.ap-south-1.amazonaws.com/kaushik-test:b2fcd6c9918431813244ec6d405a9d5a2b6b00f5",
      "portMappings": [
        {
          "containerPort": 4000,
          "hostPort": 4000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/springboot-app",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "springboot"
        }
      }
    }
  ]
}
